{
    "name": "grommasdietz/kirby-locale",
    "description": "Locale utilities for Kirby CMS",
    "type": "kirby-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Grommas Dietz",
            "email": "info@grommas-dietz.com",
            "homepage": "https://grommas-dietz.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/grommasdietz/kirby-locale/issues",
        "source": "https://github.com/grommasdietz/kirby-locale"
    },
    "keywords": [
        "kirby",
        "kirby-cms",
        "kirby-plugin",
        "locale",
        "locales"
    ],
    "autoload": {
        "psr-4": {
            "GrommasDietz\\KirbyLocale\\": "lib/"
        }
    },
    "require": {
        "getkirby/composer-installer": "^1.2"
    },
    "require-dev": {
        "getkirby/cms": "^5.0"
    },
    "config": {
        "optimize-autoloader": true,
        "allow-plugins": {
            "getkirby/composer-installer": true
        }
    },
    "extra": {
        "kirby-cms-path": false
    },
    "archive": {
        "exclude": [
            "/.editorconfig",
            "/.gitattributes",
            "/.github",
            "/.gitignore",
            "/composer.lock",
            "/package-lock.json",
            "/package.json",
            "/scripts",
            "/src"
        ]
    }
}
